
fun day1_historian_hysteria() {
    sample = """
    3   4
    4   3
    2   5
    1   3
    3   9
    3   3
    """

    rec Input(int, int)

    pairs = sample.trim().lines().map(Input::parse)

    (left, right) = pairs.unzip()
    (left, right) = (left.sort(), right.sort())

    ordered = List::zip(left, right)

    sum = ordered
        .map_to(fun(a, b) { {a - b}.abs() })
        .sum()

    assert sum == 11
}
