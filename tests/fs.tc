
fun try_open_missing_file() {
    res = File::read_all("not-a-file.txt")

    assert res.is_ok() == false
    assert res.is_err() == true
    assert res.value().is_none()
    assert res.error().is_some()

    assert res.error()! == "File not found"
}

fun try_open_this_file() {
    res = File::read_all("fs.tc")

    assert res.is_ok() == true
    assert res.is_err() == false
    assert res.value().is_some()
    assert res.error().is_none()

    assert res.value()!.trim().split(" ").at(0) == "fun"
}

